{
  "/api": {
    "target": "http://localhost:5000",
    "secure": false,
    "pathRewrite": {
      "^/api": ""
    },
    "logLevel": "debug"
  },
  "/theapi": {
    "target": "https://entsoe.herokuapp.com:443",
    "secure": false,
    "pathRewrite": {
      "^/theapi": "/api"
    },
    "logLevel": "debug",
    "changeOrigin": true,
    "headers": {
      "Connection": "keep-alive"
    }
  }
}
